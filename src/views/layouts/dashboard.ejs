<body>

    <div class="containner-fluid">

        <div class="navbar navbar-expand-lg navbar-light navIndex">
            <a href="#" class="navbar-brand text-white ml-lg-2 pt-0">
                <img class="d-inline-block rounded-pill" src="../../static/build/img/test-logo.png"
                    alt="Image C from Cadastrok">
                Cadastr<span class="border-bottom border-warning">Ok</span>
            </a>

            <form class="form-inline d-none d-lg-block mx-auto">
                <input class="form-control d-inline mt-1 mr-2" placeholder="Search" type="search" name="search"
                    id="inputSearch">
                <button type="submit" class="btn btn-outline-warning">Search</button>
            </form>

        </div>

        <ul class="nav mt-2 d-sm-block d-md-none buttons rounded-pill justify-content-center">
            <li class="nav-item">
                <button type="button" class="mt-1 mr-4 bg-dark border-0">
                    <img class="text-white" src="../../static/build/img/icon-create.png" alt="Icon Create">
                </button>
            </li>
            <li class="nav-item">
                <button type="button" class="mt-1 mr-4 bg-dark border-0">
                    <img class="text-white" src="../../static/build/img/icon-read.png" alt="Icon Read">
                </button>
            </li>
            <li class="nav-item">
                <button type="button" class="mt-1 mr-4 bg-dark border-0">
                    <img class="text-white" src="../../static/build/img/icon-update.png" alt="Icon Update">
                </button>
            </li>
            <li class="nav-item">
                <button type="button" class="mt-1 mr-4 bg-dark border-0">
                    <img class="text-white" src="../../static/build/img/icon-delete.png" alt="Icon Delete">
                </button>
            </li>
        </ul>


    </div>


    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
